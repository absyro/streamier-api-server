    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/cz-commitlint": "^19.8.0",
    "@eslint/compat": "^1.2.8",
    "@golevelup/ts-jest": "^0.6.2",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.15",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.16",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
